<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
   "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="org.walkmod:walkmod-refactor-plugin:classes" class="org.walkmod.refactor.visitors.ClassOrInterfaceRefactor"></bean>
	<bean id="org.walkmod:walkmod-refactor-plugin:methods" class="org.walkmod.refactor.visitors.MethodRefactor"></bean>

	<alias name="org.walkmod:walkmod-refactor-plugin:classes" alias="walkmod:commons:class-refactor" />
	<alias name="org.walkmod:walkmod-refactor-plugin:methods" alias="walkmod:commons:method-refactor" />
</beans>